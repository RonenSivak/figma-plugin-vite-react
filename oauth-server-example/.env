# OAuth Configuration
OAUTH_CLIENT_ID=your_client_id from the oauth provider
OAUTH_CLIENT_SECRET=your client_secret from the oauth provider

# OAuth Provider URLs
OAUTH_PROVIDER_BASE_URL=https://accounts.google.com/o/oauth2/v2/auth
OAUTH_TOKEN_URL=https://oauth2.googleapis.com/token

# Server Configuration
PORT=3001
OAUTH_REDIRECT_URI=http://localhost:3001/oauth/callback

# OAuth Scopes
OAUTH_SCOPE=openid
